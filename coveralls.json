{
    "default_stop_words": [
        "defmodule",
        "defrecord",
        "defimpl",
        "defexception",
        "defprotocol",
        "defstruct",
        "def.+(.+\\\\.+).+do",
        "^\\s+use\\s+"
    ],
    "custom_stop_words": [],
    "coverage_options": {
        "treat_no_relevant_lines_as_covered": true,
        "output_dir": "doc/cover/",
        "minimum_coverage": 90
    },
    "terminal_options": {
        "file_column_width": 70
    },
    "skip_files": []
}